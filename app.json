{
  "expo": {
    "name": "GitHub Timeline",
    "slug": "github-timeline",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "svg"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.joaovitor.githubtimeline",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.joaovitor.githubtimeline",
      "versionCode": 1,
      "backgroundColor": "#FFFFFF",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    }
  }
}
